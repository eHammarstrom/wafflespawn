{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
    },
    "rules": {
        "semi": 1
    },
    "plugins": [
        "react-native",
        "react",
    ],
    "env": {
        "browser": true
    },
    "globals": {
        "fetch": true
    }
}
